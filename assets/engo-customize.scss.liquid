      max-width: 650px;

    }
  }

  &.active{
    visibility: visible;
    opacity: 1; 
    top: 0;
    height: auto;
    width: auto; 

    .content{
      max-height: 100%;
    }
    .overlay{
      width: 100%;
      height: 100%;
    }

  }

  .close-window{
