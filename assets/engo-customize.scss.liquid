/* ========================================
11. Widgets Filter by Price, Size, Color
======================================== */

.widget-container{
  margin-bottom: 40px;
  padding-bottom: 5px;
  font-size: 14px;
  &:last-child{
    border: none;
  }

  .widget-title{
    font-size: 18px;
    margin: 0 0 30px;
    letter-spacing: 0.075em;
    .clear {
      float: right;
      line-height: 15px;
      text-transform: none;
      color: #fe5556;
      font-size: 11px;
      font-family: arial;
      font-weight: normal;
      line-height: 28px;
    }
  }

  &.widget_nav_menu,
  &.widget_categories,
  &.widget_product_categories,
  &.widget_recent_entries{
    ul{
      margin: 0;
      padding: 0;
      list-style: none;
      li{
        padding-bottom: 6px;
        &.nav-copyright{
          color: #999999;
          font-weight: 300;
        }
        input[type="checkbox"] {
          opacity:0;
          position: absolute;
        }
        a{
          color: #242626;
          text-transform:capitalize;
          &:hover{
            color: #7fc9c4;
          }
          &.active {
            color: #e0545f;
            display: block;
            position: relative;
            &:before {
              content: "";
              display: block;
              width: 9px;
              height: 1px;
              background: #e0545f;
              position: absolute;
              top: 13px;
              right: 5px;
              transform: rotateZ(45deg);
            }
            &:after {
              content: "";
              display: block;
              width: 1px;
              height: 9px;
              background: #e0545f;
              position: absolute;
              top: 9px;
              right: 9px;
              transform: rotateZ(45deg);
            }
          }
        }
        .count{
          padding-left: 17px;
          font-size: 12px;
          color: #cacaca;
        }
      }
    }
  }
  &.widget_tag_cloud{
    li {
      display: inline-block;
    }
    a{
      font-size: 14px !important;
      line-height: normal !important;
      display: inline-block;
      margin: 0 20px 20px 0;
      color: #666;
      &:hover{
        color: #000000;
      }
    }
  }

  &.widget_price_filter{
    .ui-slider {
      position: relative;
      background: #f6f6f6;
      margin-bottom: 40px;
      height: 2px;
      margin-left: 4px;
      .ui-slider-range{
        background: #000000;
        height: 100%;
        position: absolute;
      }
      .ui-slider-handle{
        height: 14px;
        width: 4px;
        background: #000000;
        position: absolute;
        top: -6px;
        cursor: ew-resize;
        margin-left: -4px;
      }
    }
  }
  &.widget-hover-content{
    .widget-hover-element{
      visibility: hidden;
      opacity: 0;
      transition: all 0.4s ease-in-out;
    }
    &:hover{
      .widget-hover-element{
        visibility: visible;
        opacity: 1;
      }
    }
  }

}

