<script>
  $('.js-banner-poster').slick({
      {% if settings.enable_rtl %}rtl: true,{% endif %}
      fade: true,
      dots: false,
      arrows: true,
      adaptiveHeight: true{% if section.settings.slider_v1_enable_autoplay %},
      autoplay: true,
      autoplaySpeed: {% if section.settings.select_slider_seconds == "slider_seconds_1" %}1000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_2" %}2000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_3" %}3000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_4" %}4000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_5" %}5000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_6" %}6000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_7" %}7000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_8" %}8000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_9" %}9000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_10" %}10000{% endif %} {% endif %}
    });

    $('.js-slide-banner-full-1').slick({
      {% if settings.enable_rtl %}rtl: true,{% endif %}
      dots: true,
      infinite: false,
      arrows: true,
      adaptiveHeight: true{% if section.settings.slider_v1_enable_autoplay %},
      autoplay: true,
      autoplaySpeed: {% if section.settings.select_slider_seconds == "slider_seconds_1" %}1000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_2" %}2000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_3" %}3000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_4" %}4000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_5" %}5000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_6" %}6000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_7" %}7000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_8" %}8000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_9" %}9000{% endif %}{% if section.settings.select_slider_seconds == "slider_seconds_10" %}10000{% endif %} {% endif %}
